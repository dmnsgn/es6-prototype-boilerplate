{
  "name": "prtt",
  "version": "4.0.1",
  "description": "A WebGL prototype starter using webpack, redux, babel, glslify, controlkit, ccapture",
  "main": "src/index.js",
  "homepage": "https://github.com/dmnsgn/prtt",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmnsgn/prtt.git"
  },
  "issues": {
    "url": "https://github.com/dmnsgn/prtt/issues"
  },
  "keywords": [
    "es2018",
    "webpack4",
    "prototype",
    "boilerplate"
  ],
  "author": "Damien Seguin <https://twitter.com/dmnsgn>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/runtime": "^7.0.0-beta.49",
    "babel-loader": "^8.0.0-beta.3",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cross-env": "^5.1.6",
    "prettier": "^1.13.2",
    "webpack": "^4.10.0",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "async-preloader": "^3.0.1",
    "ccapture.js": "^1.0.7",
    "controlkit": "^0.1.9",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "get-canvas-context": "^1.0.2",
    "gl-checker-background": "^1.0.0",
    "gl-shader": "^4.2.1",
    "glslify": "^6.1.1",
    "glslify-loader": "^1.0.2",
    "lodash": "^4.17.10",
    "query-string": "^6.1.0",
    "raf-loop": "^1.1.3",
    "raw-loader": "^0.5.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "reselect": "^3.0.1",
    "whatwg-fetch": "^2.0.4"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": false
      }
    }
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "prod": "cross-env NODE_ENV=production webpack -p"
  }
}
